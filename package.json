{
  "name": "nodejs-started",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index",
  "typings": "src/index.d.ts",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node -r multi-env/config --inspect=5000 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "test": "mocha -r ts-node/register tests/**/*.test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.6",
    "multi-env": "^2.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^12.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.3",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.7.7",
    "@types/node": "^12.12.31",
    "@types/passport": "^1.0.3",
    "chai": "^4.2.0",
    "concurrently": "^5.1.0",
    "errorhandler": "^1.5.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^24.9.0",
    "mocha": "^6.2.3",
    "nodemon": "^2.0.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.8.1",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "config": {
    "multi-env": {
      "files": [
        ".env",
        ".env.test"
      ]
    }
  }
}
